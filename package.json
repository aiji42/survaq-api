{
  "name": "survaq-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:* --print-label",
    "dev:main": "$npm_execpath run delay 3 && $npm_execpath run wrangler dev --port 8787",
    "dev:task": "wrangler dev -c wrangler.kiribi.toml --port 8788",
    "deploy": "run-s deploy:task deploy:main --print-label",
    "deploy:main": "wrangler deploy",
    "deploy:task": "wrangler deploy -c wrangler.kiribi.toml",
    "tail": "run-p tail:*",
    "tail:main": "wrangler tail",
    "tail:task": "wrangler tail -c wrangler.kiribi.toml",
    "db:pull": "prisma db pull",
    "test": "vitest",
    "build:wc": "vite build",
    "typecheck": "tsc --noEmit",
    "postinstall": "yarn prisma generate"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240423.0",
    "@types/node": "^20.12.7",
    "autoprefixer": "^10.4.19",
    "delay-cli": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "1.5.2",
    "wrangler": "^3.52.0"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.1",
    "@lit/task": "^1.0.0",
    "@prisma/adapter-pg-worker": "^5.13.0",
    "@prisma/client": "5.13.0",
    "@prisma/pg-worker": "^5.13.0",
    "csv-parse": "^5.5.5",
    "dayjs": "^1.11.11",
    "hono": "^4.3.4",
    "kiribi": "^0.0.15",
    "lit": "^3.1.3",
    "slack-cloudflare-workers": "^0.12.0"
  },
  "prettier": {
    "printWidth": 100
  }
}
